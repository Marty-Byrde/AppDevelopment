<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".messurements.MessurementsActivity">

    <Button
            android:text="Button"
            android:layout_width="12.7mm"
            android:layout_height="12.7mm" android:id="@+id/button9"
            app:layout_constraintEnd_toEndOf="@+id/button2" app:layout_constraintStart_toStartOf="@+id/button2"
            app:layout_constraintHorizontal_bias="0.463" app:layout_constraintTop_toBottomOf="@+id/button2"
            android:layout_marginTop="10dp"/>
    <TextView
            android:text="Pixels (px) [48px]"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView8"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toStartOf="@+id/button"
            android:textSize="20sp" app:layout_constraintTop_toTopOf="@+id/button"
            app:layout_constraintBottom_toBottomOf="@+id/button"/>
    <TextView
            android:text="Inches (in) [0.5in]"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView6"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toStartOf="@+id/button"
            android:textSize="20sp" app:layout_constraintTop_toTopOf="@+id/button2"
            app:layout_constraintBottom_toBottomOf="@+id/button2"/>
    <Button
            android:text="Button"
            android:layout_width="0.5in"
            android:layout_height="0.5in" android:id="@+id/button2"
            app:layout_constraintEnd_toEndOf="@+id/button" app:layout_constraintStart_toStartOf="@+id/button"
            app:layout_constraintHorizontal_bias="0.668" app:layout_constraintTop_toBottomOf="@+id/button"
            android:layout_marginTop="10dp"/>
    <Button
            android:text="Button"
            android:layout_width="48px"
            android:layout_height="48px" android:id="@+id/button"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.837" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="32dp"/>
    <TextView
            android:text="Millimeters (mm) [12.7mm]"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView5"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toStartOf="@+id/button"
            android:textSize="20sp"
            app:layout_constraintTop_toTopOf="@+id/button9" app:layout_constraintBottom_toBottomOf="@+id/button9"
            app:layout_constraintHorizontal_bias="0.125" app:layout_constraintVertical_bias="0.508"/>
    <TextView
            android:text="Points (pt) [36pt]"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView14"
            android:textSize="20sp"
            app:layout_constraintTop_toTopOf="@+id/button11" app:layout_constraintBottom_toBottomOf="@+id/button11"
            app:layout_constraintEnd_toStartOf="@+id/button11" app:layout_constraintStart_toStartOf="parent"/>
    <TextView
            android:text="Density (dp | dip) [85dp]"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView15"
            android:textSize="20sp"
            app:layout_constraintTop_toTopOf="@+id/button12" app:layout_constraintBottom_toBottomOf="@+id/button12"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toStartOf="@+id/button12"/>
    <TextView
            android:text="Scaleable Pixels (sp) [85sp]"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView16"
            android:textSize="20sp"
            app:layout_constraintTop_toTopOf="@+id/button13" app:layout_constraintBottom_toBottomOf="@+id/button13"
            app:layout_constraintEnd_toStartOf="@+id/button13" app:layout_constraintStart_toStartOf="parent"/>
    <Button
            android:text="Button"
            android:layout_width="36pt"
            android:layout_height="36pt" android:id="@+id/button11"
            app:layout_constraintTop_toBottomOf="@+id/button9" app:layout_constraintStart_toStartOf="@+id/button9"
            app:layout_constraintEnd_toEndOf="@+id/button9" android:layout_marginTop="10dp"/>
    <Button
            android:text="Button"
            android:layout_width="85dp"
            android:layout_height="85dp" android:id="@+id/button12"
            app:layout_constraintTop_toBottomOf="@+id/button11" app:layout_constraintStart_toStartOf="@+id/button11"
            app:layout_constraintEnd_toEndOf="@+id/button11" android:layout_marginTop="10dp"/>
    <Button
            android:text="Button"
            android:layout_width="85dp"
            android:layout_height="85dp" android:id="@+id/button13"
            app:layout_constraintTop_toBottomOf="@+id/button12" app:layout_constraintStart_toStartOf="@+id/button12"
            app:layout_constraintEnd_toEndOf="@+id/button12" android:layout_marginTop="10dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>